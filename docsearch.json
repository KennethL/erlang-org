{
    "index_name": "erlang_org",
    "start_urls": [
        {
            "url": "https://beta.erlang.org/doc/man/",
            "selectors_key": "man",
            "page_rank": 3,
            "tags": [
                "man"
            ]
        },
        {
            "url": "https://beta.erlang.org/doc/reference_manual/",
            "selectors_key": "refman",
            "page_rank": 2,
            "tags": [
                "refman"
            ]
        },
        {
            "url": "https://beta.erlang.org/doc/",
            "selectors_key": "guide",
            "page_rank": 1,
            "tags": [
                "guides"
            ]
        }
    ],
    "stop_urls": [
        ".*/users_guide\\.html",
        ".*/notes\\.html",
        ".*/index\\.html",
        ".*/release_notes\\.html",
        ".*/wx.*\\.html",
        ".*/gl.*\\.html",
        ".*/apps/wx/.*"
    ],
    "custom_settings": {
        "separatorsToIndex": "_"
    },
    "selectors": {
        "man": {
            "lvl0": {
                "selector": "",
                "global": true,
                "default_value": "Reference Manual"
            },
            "lvl1": ".innertube h1",
            "lvl2": ".exports-body .func-head, .exports-body .cfunc-td .title_link",
            "text": ".exports-body .exports-tube"
        },
        "refman": {
            "lvl0": {
                "selector": "",
                "global": true,
                "default_value": "Erlang Reference Manual"
            },
            "lvl1": ".innertube h1",
            "lvl2": ".innertube h3",
            "lvl3": ".innertube h4",
            "text": ".innertube p"
        },
        "guide": {
            "lvl0": {
                "selector": "",
                "global": true,
                "default_value": "User's Guide"
            },
            "lvl1": ".innertube h1",
            "lvl2": ".innertube h3",
            "lvl3": ".innertube h4",
            "text": ".innertube p"
        }
    }
}
